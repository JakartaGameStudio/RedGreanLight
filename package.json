{
  "author": "JakartaGameStudio",
  "name": "reed.green.light",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run build:prod && npm run server",
    "dev": "webpack serve --open",
    "clean": "rimraf build",
    "ssr:webpack": "webpack --mode=development",
    "ssr:server": "nodemon index.js --watch build/server.js",
    "ssr": "cross-env NODE_ENV=development npm-run-all --print-label --parallel ssr:*",
    "server": "node ./server/index.js",
    "build": "webpack",
    "build:prod": "webpack --mode=production",
    "test": "jest",
    "lint:all": "npm-run-all lint:scripts lint:ts lint:styles",
    "lint:scripts": "eslint ./ --fix",
    "lint:ts": "tsc --noEmit",
    "lint:styles": "stylelint src/**/*{.css,.scss} --fix",
    "prepare": "husky install",
    "husky:pre-commit": "npm run lint:all",
    "deploy": "npm-run-all docker:build heroku:login heroku:push heroku:release",
    "docker:build": "docker build -t web .",
    "heroku:login": "heroku login && heroku container:login",
    "heroku:push": "heroku container:push web -a=jakarta-game",
    "heroku:release": "heroku container:release web -a=jakarta-game"
  },
